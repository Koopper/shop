<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.Buyer" table="buyer" schema="public" catalog="Shop">
        <id name="idB">
            <column name="id_b" sql-type="integer"/>
        </id>
        <property name="loginB">
            <column name="login_b" sql-type="text" not-null="true"/>
        </property>
        <property name="passwordB">
            <column name="password_b" sql-type="text" not-null="true"/>
        </property>
        <property name="fioB">
            <column name="fio_b" sql-type="text" not-null="true"/>
        </property>
        <property name="emailB">
            <column name="email_b" sql-type="text" not-null="true"/>
        </property>
        <property name="phoneB">
            <column name="phone_b" sql-type="text" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>