<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entity.Sale" table="sale" schema="public" catalog="Shop">
        <id name="idDis">
            <column name="id_s" sql-type="integer"/>
        </id>
        <property name="numberS">
            <column name="number_s" sql-type="integer" not-null="true"/>
        </property>
        <property name="sum">
            <column name="sum" sql-type="real" precision="-1" not-null="true"/>
        </property>
        <property name="payment">
            <column name="payment" sql-type="boolean" not-null="true"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>